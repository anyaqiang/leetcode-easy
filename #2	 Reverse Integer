class Solution {
    public int reverse(int x) {
        int y=0,s=-1;
        double sum=0;
        if(x>Integer.MAX_VALUE||x<Integer.MIN_VALUE)
            s=0;
        else{
          while(x!=0){
             y=x%10;
             sum=sum*10+y;
             x=x/10;
          }     
        }
        if(sum>Integer.MAX_VALUE||sum<Integer.MIN_VALUE)
            s=0;
        else
            s=(int)sum;
        
    return s;
        
    }
}
